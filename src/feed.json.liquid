---
permalink: /feed.json
---
[
  {% for post in collections.posts %}
    {
      "title": "{{ post.data.title }}",
      "url": "{{ post.url }}",
      "date": "{{ post.date | date: '%Y-%m-%d' }}",
      "tags": {{ post.data.tags | json }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
